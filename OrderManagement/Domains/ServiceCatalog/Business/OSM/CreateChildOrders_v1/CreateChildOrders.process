        <pd:inputBindings>
            <root>
                <NrCreatedOrders>
                    <xsl:value-of select="count($FindOrders/Output/pfx2:ResponseEAI/pfx2:Body/ns1:FindOrderResponse/ns1:Order)"/>
                </NrCreatedOrders>
            </root>
        </pd:inputBindings>
    </pd:activity>
