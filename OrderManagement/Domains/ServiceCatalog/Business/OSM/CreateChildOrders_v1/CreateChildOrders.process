    </pd:transition>
    <pd:transition>
        <pd:from>FindOrders</pd:from>
        <pd:to>Map FindOrders Res</pd:to>
        <pd:xpathDescription/>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Parse-XML</pd:from>
        <pd:to>FindOrders</pd:to>
        <pd:xpathDescription>Resubmit</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Start/Input/pfx:RequestEAI/pfx:Body/ns6:Request/ns6:Resubmit = 'Y'
or
(string-length($Start/Input/pfx:RequestEAI/pfx:Header/ns2:Tries) &gt; 0
and
number($Start/Input/pfx:RequestEAI/pfx:Header/ns2:Tries) &gt; 0)</pd:xpath>
