        <pd:x>315</pd:x>
        <pd:y>195</pd:y>
        <config>
            <element>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element name="NrCreatedOrders" type="xsd:string" minOccurs="0"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:coercions>
            <pd:coercion xpath="$FindOrders/Output/pfx2:ResponseEAI/pfx2:Body/*" element="ns1:FindOrderResponse"/>
