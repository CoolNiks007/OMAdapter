    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>462</pd:x>
        <pd:y>67</pd:y>
        <config>
            <element ref="pfx4:Request"/>
        </config>
        <pd:inputBindings>
            <xsl:variable name="rootItem" select="if(count($Parse-XML/Order/ListOfRootItem/RootItem[ComponentSubType= 'MOBILE'])=1)&#xA;then $Parse-XML/Order/ListOfRootItem/RootItem[ComponentSubType= 'MOBILE']&#xA;else&#xA;(&#x9;&#xA;&#x9;if(count($Parse-XML/Order/ListOfRootItem/RootItem[ComponentSubType= 'MOBILE'])=0)&#xA;&#x9;then&#xA;&#x9;$Parse-XML/Order/ListOfRootItem/RootItem&#xA;&#x9;else&#xA;&#x9;(&#xA;&#x9;&#x9;if($Start/Input/pfx:RequestEAI/pfx:Body/ns:Request/ns:OrderComponentDetails/ns:Name = 'ProvisioningManagement.ExternalTable.AddVPNShortNumber')&#xA;&#x9;&#x9;then&#xA;&#x9;&#x9;$Parse-XML/Order/ListOfRootItem/RootItem[ComponentSubType= 'MOBILE' and Action= 'New']&#xA;&#x9;&#x9;else&#xA;&#x9;&#x9;$Parse-XML/Order/ListOfRootItem/RootItem[ComponentSubType= 'MOBILE' and Action= 'Deleted']&#xA;&#x9;)&#xA;)"/>
            <xsl:variable name="action" select="if($Start/Input/pfx:RequestEAI/pfx:Body/ns:Request/ns:OrderComponentDetails/ns:Name = 'ProvisioningManagement.ExternalTable.AddVPNShortNumber')&#xA;then&#xA;'Burn'&#xA;else&#xA;'Release'"/>
            <pfx4:Request>
                <pfx4:ListOfShortNumbers>
                    <xsl:choose>
                        <xsl:when test="$rootItem/ProductCatalogId !=$_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/ProductCatalogId/VPN_Group">
                            <xsl:for-each select="if($action = 'Burn')&#xA;then&#xA;$rootItem/ListOfChildItem/ChildItem[(ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/Action = ('Modified','New') or ListOfChildItemAttribute/ChildItemAttribute[Name = 'Group #']/Action = ('Modified','New') ) and Action = ('New','Modified') and ProductCatalogId = $_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/ProductCatalogId/VPN]&#xA;else&#xA;$rootItem/ListOfChildItem/ChildItem[(ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/Action = ('Modified','Deleted') or ListOfChildItemAttribute/ChildItemAttribute[Name = 'Group #']/Action = ('Modified','Deleted') ) and Action = ('Deleted','Modified') and ProductCatalogId = $_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/ProductCatalogId/VPN]">
                                <pfx4:ShortNumber>
                                    <xsl:choose>
                                        <xsl:when test="$action='Burn'">
                                    <pfx4:VPNGroupID>
                                        <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Group #']/Value"/>
                                    </pfx4:VPNGroupID>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <pfx4:VPNGroupID>
                                                <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Group #']/PreviousValue"/>
                                            </pfx4:VPNGroupID>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <xsl:choose>
                                        <xsl:when test="$action='Burn'">
                                    <xsl:if test="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/Value">
                                        <pfx4:ShortNumber>
                                            <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/Value"/>
                                        </pfx4:ShortNumber>
                                    </xsl:if>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <pfx4:ShortNumber>
                                                <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/PreviousValue"/>
                                            </pfx4:ShortNumber>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <xsl:choose>
                                        <xsl:when test="$action='Burn'">
                                    <xsl:if test="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number Length']/Value">
                                        <pfx4:ShortNumberLength>
                                            <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number Length']/Value"/>
                                        </pfx4:ShortNumberLength>
                                    </xsl:if>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <pfx4:ShortNumberLength>
                                                <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number Length']/PreviousValue"/>
                                            </pfx4:ShortNumberLength>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <xsl:if test="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/PreviousValue !=ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/Value&#xA;and&#xA;not(ListOfChildItemAttribute/ChildItemAttribute[Name = 'Group #']/PreviousValue !=ListOfChildItemAttribute/ChildItemAttribute[Name = 'Group #']/Value)">
                                        <xsl:if test="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/PreviousValue">
                                            <pfx4:OldShortNumber>
                                                <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/PreviousValue"/>
                                            </pfx4:OldShortNumber>
                                        </xsl:if>
                                    </xsl:if>
                                    <pfx4:MSISDN>
                                        <xsl:value-of select="$rootItem/ServiceId"/>
                                    </pfx4:MSISDN>
                                    <pfx4:ReservationEntity>
                                        <xsl:value-of select="'ORDER'"/>
                                    </pfx4:ReservationEntity>
                                    <xsl:if test="$rootItem/IntegrationId">
                                        <pfx4:ReservationID>
                                            <xsl:value-of select="$rootItem/IntegrationId"/>
                                        </pfx4:ReservationID>
                                    </xsl:if>
                                    <pfx4:AssetIntegrationID>
                                        <xsl:value-of select="IntegrationId"/>
                                    </pfx4:AssetIntegrationID>
                                </pfx4:ShortNumber>
                            </xsl:for-each>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:for-each select="if($action = 'Burn')&#xA;then&#xA;$rootItem/ListOfChildItem/ChildItem[ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/Action = ('Modified','New') and Action = ('New','Modified') and ProductCatalogId = $_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/ProductCatalogId/PSTN]&#xA;else&#xA;$rootItem/ListOfChildItem/ChildItem[ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/Action =('Deleted') and Action =('Deleted') and ProductCatalogId = $_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/ProductCatalogId/PSTN]">
                                <pfx4:ShortNumber>
                                    <xsl:choose>
                                        <xsl:when test="$action='Burn'">
                                    <pfx4:VPNGroupID>
                                        <xsl:value-of select="$rootItem/ListOfRootItemAttribute/RootItemAttribute[Name = 'Group #']/Value"/>
                                    </pfx4:VPNGroupID>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <pfx4:VPNGroupID>
                                                <xsl:value-of select="$rootItem/ListOfRootItemAttribute/RootItemAttribute[Name = 'Group #']/PreviousValue"/>
                                            </pfx4:VPNGroupID>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <xsl:choose>
                                        <xsl:when test="$action='Burn'">
                                    <pfx4:ShortNumber>
                                        <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/Value"/>
                                    </pfx4:ShortNumber>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <pfx4:ShortNumber>
                                                <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/PreviousValue"/>
                                            </pfx4:ShortNumber>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <xsl:choose>
                                        <xsl:when test="$action='Burn'">
                                    <pfx4:ShortNumberLength>
                                                <xsl:value-of select="$rootItem/ListOfRootItemAttribute/RootItemAttribute[Name = 'Short Number Length']/Value"/>
                                            </pfx4:ShortNumberLength>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <pfx4:ShortNumberLength>
                                        <xsl:value-of select="$rootItem/ListOfRootItemAttribute/RootItemAttribute[Name = 'Short Number Length']/PreviousValue"/>
                                    </pfx4:ShortNumberLength>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <xsl:if test="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/PreviousValue !=ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/Value&#xA;and&#xA;not(ListOfChildItemAttribute/ChildItemAttribute[Name = 'Group #']/PreviousValue !=ListOfChildItemAttribute/ChildItemAttribute[Name = 'Group #']/Value)">
                                        <pfx4:OldShortNumber>
                                            <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name = 'Short Number']/PreviousValue"/>
                                        </pfx4:OldShortNumber>
                                    </xsl:if>
                                    <pfx4:ReservationEntity>
                                        <xsl:value-of select="'ORDER'"/>
                                    </pfx4:ReservationEntity>
                                    <xsl:if test="$rootItem/IntegrationId">
                                        <pfx4:ReservationID>
                                            <xsl:value-of select="$rootItem/IntegrationId"/>
                                        </pfx4:ReservationID>
                                    </xsl:if>
                                    <pfx4:AssetIntegrationID>
                                        <xsl:value-of select="IntegrationId"/>
                                    </pfx4:AssetIntegrationID>
                                </pfx4:ShortNumber>
                            </xsl:for-each>
                        </xsl:otherwise>
                    </xsl:choose>
                </pfx4:ListOfShortNumbers>
                        <pfx4:Action>
                    <xsl:value-of select="$action"/>
                        </pfx4:Action>
                <pfx4:UserID>
                    <xsl:value-of select="$Parse-XML/Order/OrderCreator"/>
                </pfx4:UserID>
            </pfx4:Request>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="JMS Queue Sender">
