                                        </ns32:StartDate>
                                    </xsl:if>
                                    <xsl:choose>
                                        <xsl:when test="string-length($OldRootItem/ListOfRootItemAttribute/RootItemAttribute[Name='Donor Operator']/Value)>0">
                                            <ns32:MNP>
                                                <xsl:value-of select="true()"/>
                                            </ns32:MNP>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <ns32:MNP>
                                                <xsl:value-of select="false()"/>
                                            </ns32:MNP>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                </ns32:OldSubscriber>
                            </ns32:Request>
                        </pfx5:Body>
                    </pfx5:ServiceRequest>
                </Body>
            </ns2:ActivityInput>
        </pd:inputBindings>
