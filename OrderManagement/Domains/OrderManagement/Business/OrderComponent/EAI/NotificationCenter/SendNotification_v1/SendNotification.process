                                                    </ns7:Value>
                                                </ns7:Token>
                                            </xsl:for-each>
                                        </ns7:Template>
                                        <ns7:ValidityPeriod>
                                            <ns7:Days>
                                                <xsl:value-of select="1"/>
                                            </ns7:Days>
                                            <ns7:Hours>
                                                <xsl:value-of select="'12:15:00'"/>
                                            </ns7:Hours>
                                        </ns7:ValidityPeriod>
                                    </pfx3:Notification>
                                </xsl:for-each>
                            </xsl:when>
                            <xsl:when test="exists($GetBundleConsumable/ns2:ActivityOutput/Body/ns22:ServiceReply/ns22:Body/ns8:Response/ns8:ListOfAssets)">
                                <xsl:variable name="Attributes" select="$GetBundleConsumable/ns2:ActivityOutput/Body/ns22:ServiceReply/ns22:Body/ns8:Response/ns8:ListOfAssets"/>
                                <pfx3:Notification>
                                    <ns7:Channel>
                                        <ns7:Id>
                                            <xsl:value-of select="$Template/Channel"/>
                                        </ns7:Id>
                                    </ns7:Channel>
                                    <ns7:Source>
                                        <xsl:value-of select="'OSM'"/>
                                    </ns7:Source>
                                    <xsl:choose>
                                        <xsl:when test="string-length($Template/DestinationEval) >0">
                                            <ns7:Destination>
                                                <xsl:value-of select="tib:evaluate($Template/DestinationEval)[1]"/>
                                            </ns7:Destination>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <ns7:Destination>
                                                <xsl:value-of select="$RootItem/ServiceId"/>
                                            </ns7:Destination>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <ns7:Message>
                                        <xsl:choose>
                                            <xsl:when test="$Start/Input/pfx:RequestEAI/pfx:Body/ns:Request/ns:OrderComponentDetails/ns:Scenario = 'NotificationCenter.NotifyFutureDatedOrderSchedule.Service.Common.FutureOrder.Service.Common.FutureOrder.Base'">
                                                <ns7:Type>
                                                    <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/WhiteHours"/>
                                                </ns7:Type>
                                            </xsl:when>
                                            <xsl:otherwise>
                                                <xsl:if test="string-length($Template/TypeId)>0">
