        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>1230</pd:x>
        <pd:y>515</pd:y>
        <config>
            <element ref="pfx3:Request"/>
        </config>
        <pd:coercions>
            <pd:coercion xpath="$GetBundleConsumable/ns2:ActivityOutput/Body/ns22:ServiceReply/ns22:Body/*" element="ns8:Response"/>
            <pd:coercion xpath="$GetBundleMSISDNList/ns2:ActivityOutput/Body/ns22:ServiceReply/ns22:Body/*" element="ns9:Response"/>
        </pd:coercions>
        <pd:inputBindings>
            <xsl:variable name="Language" select="'TRK'"/>
            <pfx3:Request>
                <xsl:variable name="Order" select="$Parse-XML/Order"/>
                <xsl:variable name="RootItem" select="if (count($Parse-XML/Order/ListOfRootItem/RootItem[ProductCatalogId=('1000','70000')])>0) then $Parse-XML/Order/ListOfRootItem/RootItem[ProductCatalogId=('1000','70000')]&#xA;else $Parse-XML/Order/ListOfRootItem/RootItem[1]"/>
                <pfx3:NotificationList>
                    <!--General-->
                    <xsl:for-each select="$GetTemplates/ListOfTemplates/Template[SVBFlag = 'N' or string-length(SVBFlag)=0 ]">
                        <xsl:variable name="ProductCatalogId" select="ProductCatalogId"/>
                        <pfx3:Notification>
                            <ns7:Channel>
                                <ns7:Id>
                                    <xsl:value-of select="Channel"/>
                                </ns7:Id>
                            </ns7:Channel>
                            <xsl:if test="$Order/OrderType = 'Missing Documents' and Channel = 'AUTODIALER_TTS'">
                                <ns7:ChannelAttributes>
                                    <ns7:Name>
                                        <xsl:value-of select="'PLATFORM'"/>
                                    </ns7:Name>
                                    <ns7:Value>
                                        <xsl:value-of select="$Order/OrderOMSourceSystem"/>
                                    </ns7:Value>
                                </ns7:ChannelAttributes>
                            </xsl:if>
                            <xsl:if test="$Order/OrderOMSourceSystem">
                                <ns7:ChannelSource>
                                    <xsl:value-of select="$Order/OrderOMSourceSystem"/>
                                </ns7:ChannelSource>
                            </xsl:if>
                            <ns7:Source>
                                <xsl:value-of select="'OSM'"/>
                            </ns7:Source>
                            <xsl:choose>
                                <!--SVB roots-->
                                <xsl:when test="string-length(DestinationEval) >0">
                                    <ns7:Destination>
                                        <xsl:value-of select="tib:evaluate(DestinationEval)[1]"/>
                                    </ns7:Destination>
                                </xsl:when>
                                <xsl:when test="current()/Channel = &quot;EMAIL&quot;">
                                    <xsl:for-each select="tib:tokenize($Order/ListOfCustomerAccount/CustomerAccount[1]/CustAcctEmail, &quot;;&quot;)">
                                        <ns7:Destination>
                                            <xsl:value-of select="current()"/>
                                        </ns7:Destination>
                                    </xsl:for-each>
                                </xsl:when>
                                <xsl:when test="string-length($RootItem/ListOfRootItemAttribute/RootItemAttribute[Name ='Notification Number_1']/Value)>0">
                                    <ns7:Destination>
                                        <xsl:value-of select="$RootItem/ListOfRootItemAttribute/RootItemAttribute[Name ='Notification Number_1']/Value"/>
                                    </ns7:Destination>
                                </xsl:when>
                                <xsl:when test="count($RootItem[ProductCatalogId=('1000','70000')])>0">
                                    <ns7:Destination>
                                        <xsl:value-of select="$RootItem[ProductCatalogId=('1000','70000')]/ServiceId"/>
                                    </ns7:Destination>
                                </xsl:when>
                                <xsl:otherwise>
                                    <ns7:Destination>
                                        <xsl:value-of select="$RootItem[1]/ServiceId"/>
                                    </ns7:Destination>
                                </xsl:otherwise>
                            </xsl:choose>
                            <ns7:Message>
                                <xsl:choose>
                                    <xsl:when test="$Start/Input/pfx:RequestEAI/pfx:Body/ns:Request/ns:OrderComponentDetails/ns:Scenario = 'NotificationCenter.NotifyFutureDatedOrderSchedule.Service.Common.FutureOrder.Service.Common.FutureOrder.Base'">
                                        <ns7:Type>
                                            <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/WhiteHours"/>
                                        </ns7:Type>
                                    </xsl:when>
                                    <xsl:otherwise>
                                        <xsl:if test="string-length(TypeId)>0">
                                            <xsl:if test="TypeId">
                                                <ns7:Type>
                                                    <xsl:value-of select="TypeId"/>
                                                </ns7:Type>
                                            </xsl:if>
                                        </xsl:if>
                                    </xsl:otherwise>
                                </xsl:choose>
                            </ns7:Message>
                            <ns7:Template>
                                <ns7:Id>
                                    <xsl:value-of select="Id"/>
                                </ns7:Id>
                                <ns7:Language>
                                    <xsl:value-of select="$Language"/>
                                </ns7:Language>
                                <xsl:for-each select="ListOfTokens/Token">
                                    <ns7:Token>
                                        <ns7:Name>
                                            <xsl:value-of select="Name"/>
                                        </ns7:Name>
                                        <ns7:Value>
                                            <xsl:value-of select="tib:evaluate(Xpath)[1]"/>
                                        </ns7:Value>
                                    </ns7:Token>
                                </xsl:for-each>
                            </ns7:Template>
                            <ns7:ValidityPeriod>
                                <ns7:Days>
                                    <xsl:value-of select="1"/>
                                </ns7:Days>
                                <ns7:Hours>
                                    <xsl:value-of select="'12:15:00'"/>
                                </ns7:Hours>
                            </ns7:ValidityPeriod>
                        </pfx3:Notification>
                    </xsl:for-each>
                    <!--SVB Root notify all MSISDN´s-->
                    <xsl:for-each select="$GetTemplates/ListOfTemplates/Template[SVBFlag = 'Y']">
                        <xsl:variable name="Template" select="."/>
                        <xsl:choose>
                            <xsl:when test="exists($GetBundleMSISDNList/ns2:ActivityOutput/Body/ns22:ServiceReply/ns22:Body/ns9:Response/ns9:ListOfAssetsHeader/ns9:MSISDN) and string-length($GetBundleMSISDNList/ns2:ActivityOutput/Body/ns22:ServiceReply/ns22:Body/ns9:Response/ns9:ListOfAssetsHeader[1]/ns9:MSISDN)">
                                <xsl:for-each select="$GetBundleMSISDNList/ns2:ActivityOutput/Body/ns22:ServiceReply/ns22:Body/ns9:Response/ns9:ListOfAssetsHeader">
                                    <xsl:variable name="Attributes" select="ns9:ListOfAssets"/>
                                    <pfx3:Notification>
                                        <ns7:Channel>
                                            <ns7:Id>
                                                <xsl:value-of select="$Template/Channel"/>
                                            </ns7:Id>
                                        </ns7:Channel>
                                        <ns7:Source>
                                            <xsl:value-of select="'OSM'"/>
                                        </ns7:Source>
                                        <xsl:choose>
                                            <xsl:when test="string-length($Template/DestinationEval) >0">
                                                <ns7:Destination>
                                                    <xsl:value-of select="tib:evaluate($Template/DestinationEval)[1]"/>
                                                </ns7:Destination>
                                            </xsl:when>
                                            <xsl:otherwise>
                                                <ns7:Destination>
                                                    <xsl:value-of select="ns9:MSISDN"/>
                                                </ns7:Destination>
                                            </xsl:otherwise>
                                        </xsl:choose>
                                        <ns7:Message>
                                            <xsl:choose>
                                                <xsl:when test="$Start/Input/pfx:RequestEAI/pfx:Body/ns:Request/ns:OrderComponentDetails/ns:Scenario = 'NotificationCenter.NotifyFutureDatedOrderSchedule.Service.Common.FutureOrder.Service.Common.FutureOrder.Base'">
                                                    <ns7:Type>
                                                        <xsl:value-of select="$_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/WhiteHours"/>
                                                    </ns7:Type>
                                                </xsl:when>
                                                <xsl:otherwise>
                                                    <xsl:if test="string-length($Template/TypeId)>0">
