                                    <xsl:variable name="custAcctId" select="../../CustAcctCode"/>
                                    <Action>
                                        <xsl:value-of select="Action"/>
                                    </Action>
                                    <MNPCode/>
                                    <OrderTypeCode>
                                        <xsl:value-of select="../../../../OrderType"/>
                                    </OrderTypeCode>
                                    <ProductCatalogId>
                                        <xsl:value-of select="ProductCatalogId"/>
                                    </ProductCatalogId>
                                    <ActionType>
                                        <xsl:value-of select="../../ActionType"/>
                                    </ActionType>
                                    <CustomerType>
                                        <xsl:value-of select="../../../../ListOfCustomerAccount/CustomerAccount[CustAcctCode = $custAcctId ]/CustAcctType"/>
                                    </CustomerType>
                                    <ActionReason>
                                        <xsl:value-of select="../../ActionReason"/>
                                    </ActionReason>
                                    <xsl:choose>
                                        <xsl:when test="string-length(../ChildItem[ProductCatalogId = $_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/ProductCatalogId/SIM]/DunningSMSFlag) >0">
                                            <DunningSMSFlag>
                                                <xsl:value-of select="../ChildItem[ProductCatalogId = $_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/ProductCatalogId/SIM]/DunningSMSFlag"/>
                                            </DunningSMSFlag>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <DunningSMSFlag/>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <xsl:choose>
                                        <xsl:when test="string-length(ListOfChildItemAttribute/ChildItemAttribute[Name='SVB Threshold SMS Notification Flag']/Value)>0">
                                            <SVBFlag>
                                                <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name='SVB Threshold SMS Notification Flag']/Value"/>
                                            </SVBFlag>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <SVBFlag>
                                                <xsl:value-of select="''"/>
                                            </SVBFlag>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <ComponentGroup>
                                        <xsl:value-of select="ComponentGroup"/>
                                    </ComponentGroup>
                                    <ComponentType>
                                        <xsl:value-of select="ComponentType"/>
                                    </ComponentType>
                                    <ComponentSubType>
                                        <xsl:value-of select="ComponentSubType"/>
                                    </ComponentSubType>
                                </Product>
                            </xsl:when>
                            <xsl:when test="not(../../ActionType = ('127','128'))">
                                <Product>
                                    <xsl:variable name="custAcctId" select="../../CustAcctCode"/>
                                    <Action>
                                        <xsl:value-of select="Action"/>
                                    </Action>
                                    <MNPCode/>
                                    <OrderTypeCode>
                                        <xsl:value-of select="../../../../OrderType"/>
                                    </OrderTypeCode>
                                    <ProductCatalogId>
                                        <xsl:value-of select="ProductCatalogId"/>
                                    </ProductCatalogId>
                                    <ActionType>
                                        <xsl:value-of select="../../ActionType"/>
                                    </ActionType>
                                    <CustomerType>
                                        <xsl:value-of select="../../../../ListOfCustomerAccount/CustomerAccount[CustAcctCode = $custAcctId ]/CustAcctType"/>
                                    </CustomerType>
                                    <ActionReason>
                                        <xsl:value-of select="../../ActionReason"/>
                                    </ActionReason>
                                    <xsl:choose>
                                        <xsl:when test="string-length(../ChildItem[ProductCatalogId = $_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/ProductCatalogId/SIM]/DunningSMSFlag) >0">
                                            <DunningSMSFlag>
                                                <xsl:value-of select="../ChildItem[ProductCatalogId = $_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/ProductCatalogId/SIM]/DunningSMSFlag"/>
                                            </DunningSMSFlag>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <DunningSMSFlag/>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <xsl:choose>
                                        <xsl:when test="string-length(ListOfChildItemAttribute/ChildItemAttribute[Name='SVB Threshold SMS Notification Flag']/Value)>0">
                                            <SVBFlag>
                                                <xsl:value-of select="ListOfChildItemAttribute/ChildItemAttribute[Name='SVB Threshold SMS Notification Flag']/Value"/>
                                            </SVBFlag>
                                        </xsl:when>
                                        <xsl:otherwise>
                                            <SVBFlag>
                                                <xsl:value-of select="''"/>
                                            </SVBFlag>
                                        </xsl:otherwise>
                                    </xsl:choose>
                                    <ComponentGroup>
                                        <xsl:value-of select="ComponentGroup"/>
                                    </ComponentGroup>
                                    <ComponentType>
                                        <xsl:value-of select="ComponentType"/>
                                    </ComponentType>
                                    <ComponentSubType>
                                        <xsl:value-of select="ComponentSubType"/>
                                    </ComponentSubType>
                                </Product>
                            </xsl:when>
                        </xsl:choose>
                    </xsl:for-each>
                    <xsl:for-each select="$Parse-XML/Order[OrderType!='Missing Documents']/ListOfRootItem/RootItem">
                        <Product>
                            <xsl:variable name="custAcctId" select="CustAcctCode"/>
                            <Action>
                                <xsl:value-of select="Action"/>
                            </Action>
                            <MNPCode/>
                            <OrderTypeCode>
                                <xsl:value-of select="../../OrderType"/>
                            </OrderTypeCode>
                            <ProductCatalogId>
                                <xsl:value-of select="ProductCatalogId"/>
                            </ProductCatalogId>
                            <ActionType>
                                <xsl:value-of select="ActionType"/>
                            </ActionType>
                            <CustomerType>
                                <xsl:value-of select="../../ListOfCustomerAccount/CustomerAccount[CustAcctCode = $custAcctId ]/CustAcctType"/>
                            </CustomerType>
                            <ActionReason>
                                <xsl:value-of select="ActionReason"/>
                            </ActionReason>
                            <xsl:choose>
                                <xsl:when test="string-length(ListOfChildItem/ChildItem[ProductCatalogId = $_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/ProductCatalogId/SIM]/DunningSMSFlag) >0">
                                    <DunningSMSFlag>
                                        <xsl:value-of select="ListOfChildItem/ChildItem[ProductCatalogId = $_globalVariables/ns1:GlobalVariables/Domains/OrderManagement/OrderComponent/Constants/ProductCatalogId/SIM]/DunningSMSFlag"/>
                                    </DunningSMSFlag>
                                </xsl:when>
                                <xsl:otherwise>
                                    <DunningSMSFlag/>
                                </xsl:otherwise>
                            </xsl:choose>
                            <xsl:choose>
                                <xsl:when test="$Parse-XML/Order/ListOfRootItem/RootItem/ListOfRootItemAttribute/RootItemAttribute[Name='SVB Bundle SMS Notification Flag']/Value='Y'&#xA;&#x9;or&#xA;&#x9;$Parse-XML/Order/ListOfRootItem/RootItem/ListOfChildItem/ChildItem[ProductCatalogId =('4010','7010','8010','9010','10010','30010')]/ListOfChildItemAttribute/ChildItemAttribute[Name='SVB Bundle SMS Notification Flag']/Value='Y'">
                                    <SVBFlag>
                                        <xsl:value-of select="'Y'"/>
                                    </SVBFlag>
                                </xsl:when>
                                <xsl:when test="$Parse-XML/Order/ListOfRootItem/RootItem/ListOfRootItemAttribute/RootItemAttribute[Name='SVB Bundle SMS Notification Flag']/Value='N'&#xA;&#x9;or&#xA;&#x9;$Parse-XML/Order/ListOfRootItem/RootItem/ListOfChildItem/ChildItem[ProductCatalogId =('4010','7010','8010','9010','10010','30010')]/ListOfChildItemAttribute/ChildItemAttribute[Name='SVB Bundle SMS Notification Flag']/Value='N'">
                                    <SVBFlag>
                                        <xsl:value-of select="'N'"/>
                                    </SVBFlag>
                                </xsl:when>
                                <xsl:otherwise>
                                    <SVBFlag>
                                        <xsl:value-of select="''"/>
                                    </SVBFlag>
                                </xsl:otherwise>
                            </xsl:choose>
                            <ComponentGroup>
                                <xsl:value-of select="ComponentGroup"/>
                            </ComponentGroup>
                            <ComponentType>
                                <xsl:value-of select="ComponentType"/>
                            </ComponentType>
                            <ComponentSubType>
                                <xsl:value-of select="ComponentSubType"/>
                            </ComponentSubType>
                        </Product>
                    </xsl:for-each>
                </Products>
            </MapProducts>
        </pd:inputBindings>
