                                                            <ns7:Type>
                                                                <xsl:value-of select="$Template/TypeId"/>
                                                            </ns7:Type>
                                                        </xsl:if>
                                                    </xsl:if>
                                                </xsl:otherwise>
                                            </xsl:choose>
                                        </ns7:Message>
                                        <ns7:Template>
                                            <ns7:Id>
                                                <xsl:value-of select="$Template/Id"/>
                                            </ns7:Id>
                                            <ns7:Language>
                                                <xsl:value-of select="$Language"/>
                                            </ns7:Language>
                                            <xsl:for-each select="$Template/ListOfTokens/Token">
                                                <ns7:Token>
                                                    <ns7:Name>
                                                        <xsl:value-of select="Name"/>
                                                    </ns7:Name>
                                                    <ns7:Value>
